  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred",
  "share_target": {
    "action": "/create",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "text": "job_title",
      "url": "website"
    }
  },
  "file_handlers": [
    {
      "action": "/batch",
      "accept": {
        "text/csv": [".csv"]
      },
      "launch_type": "single-client"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+businesscard",
      "url": "/create?data=%s"
    }
  ]
}
