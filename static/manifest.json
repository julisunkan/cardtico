      "name": "Create New Card",
      "short_name": "Create",
      "description": "Start creating a new business card",
      "url": "/create",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Batch Processing",
      "short_name": "Batch",
      "description": "Process multiple cards from CSV",
      "url": "/batch",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
